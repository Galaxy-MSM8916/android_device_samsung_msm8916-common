    class late_start
    group gps inet net_raw diag net_admin wifi

service vendor.fm /system/vendor/bin/init.qcom.fm.sh
    class late_start
    user system
    group system
    disabled
    oneshot

service qcom-sh /system/vendor/bin/init.qcom.sh
    class late_start
    user root
    oneshot

service qcom-early-boot /system/vendor/bin/init.qcom.early_boot.sh
